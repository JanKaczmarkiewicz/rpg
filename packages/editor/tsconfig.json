{
    "extends": "../../tsconfig.json",
    "compilerOptions": {
        "resolveJsonModule": true,
        "lib": ["dom", "dom.iterable", "esnext"],
        "noEmit": true,
        "esModuleInterop": true,
        "moduleResolution": "node",
        "jsx": "preserve",
        "allowJs": true,
        "skipLibCheck": true,
        "module": "esnext",
        "isolatedModules": true
    },
    "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx"],
    "exclude": ["node_modules"]
}
